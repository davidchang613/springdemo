def map = [:]
def modules = [:]

pipeline {
  agent any

  stages {
    stage('load file') {
      steps {
        script {
          modules.shared = load "./jenkins/lib/shared.groovy"
          modules.shared.getSelf()

          modules.shared.readVariablesFromFile("config.txt", map)
          echo map['var1']
        }
      }
    }
  }
}

